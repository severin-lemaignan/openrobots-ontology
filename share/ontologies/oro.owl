                        <owl:hasValue rdf:datatype="&xsd;boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <openCycUri>http://sw.opencyc.org/concept/Mx4rwJaXepwpEbGdrcN5Y29ycA</openCycUri>
    </owl:Class>
    


