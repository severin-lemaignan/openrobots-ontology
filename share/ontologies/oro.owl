    <owl:ObjectProperty rdf:about="&concept;above-Generally"/>
    


    <!-- http://sw.opencyc.org/concept/above_Touching -->

    <owl:ObjectProperty rdf:about="&concept;above_Touching"/>
    


    <!-- http://sw.opencyc.org/concept/behind-Generally -->

    <owl:ObjectProperty rdf:about="&concept;behind-Generally"/>
    


    <!-- http://sw.opencyc.org/concept/bodilyDoer -->

    <owl:ObjectProperty rdf:about="&concept;bodilyDoer">
        <rdfs:comment xml:lang="en">The predicate bodilyDoer relates a given physical event to any organism that is a non-deliberate &quot;doer&quot; of the event (i.e. the organism acts without conscious intention or volition).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&concept;doneBy"/>
    </owl:ObjectProperty>
    


    <!-- http://sw.opencyc.org/concept/doneBy -->

    <owl:ObjectProperty rdf:about="&concept;doneBy">
        <rdfs:comment xml:lang="en">A very general predicate that relates events to the things that &quot;do&quot; them. (doneBy EVENT DOER) means that DOER is a &quot;doer&quot; in the event EVENT: some activity on the part of DOER causes or carries out EVENT. This predicate is agnostic as to whether DOER does EVENT intentionally or not. Note that DOER need not even be animate or an agent; e.g. the famous vocanic eruption that buried Pompeii was doneBy Mt. Vesuvius. Note also that an event might have more than one doer; consider a chess game.</rdfs:comment>
        <rdfs:domain rdf:resource="&concept;Event"/>
    </owl:ObjectProperty>
    


    <!-- http://sw.opencyc.org/concept/farFrom -->

    <owl:ObjectProperty rdf:about="&concept;farFrom">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label>is far from</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Location"/>
        <owl:propertyDisjointWith rdf:resource="&concept;near"/>
    </owl:ObjectProperty>
    


