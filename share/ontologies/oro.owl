    <!-- http://kb.openrobots.org#actionInvolves -->

    <owl:ObjectProperty rdf:about="&oro;actionInvolves">
        <rdfs:subPropertyOf rdf:resource="&oro;involves"/>
        <rdfs:domain rdf:resource="&concept;Action"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#actsOnObject -->

    <owl:ObjectProperty rdf:about="&oro;actsOnObject">
        <rdfs:label xml:lang="en">acts on</rdfs:label>
        <rdfs:label xml:lang="fr">agit sur</rdfs:label>
        <rdfs:comment>Theme (or Patient) of an action: Entity undergoing the effect of some action

Example: &gt;Mary&lt; fell over.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;hasThematicRoleRelationWith"/>
        <rdfs:range rdf:resource="&concept;PartiallyTangible"/>
        <rdfs:domain rdf:resource="&concept;PurposefulAction"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#after -->

    <owl:ObjectProperty rdf:about="&oro;after">
        <rdfs:domain rdf:resource="&oro;TimePoint"/>
        <rdfs:range rdf:resource="&oro;TimePoint"/>
        <rdfs:subPropertyOf rdf:resource="&oro;after-Underspecified"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#after-Underspecified -->

    <owl:ObjectProperty rdf:about="&oro;after-Underspecified">
        <rdfs:subPropertyOf rdf:resource="&oro;temporallyRelated"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#agentRelativeProperty -->

    <owl:ObjectProperty rdf:about="&oro;agentRelativeProperty">
        <rdfs:comment xml:lang="en">An &quot;agent relative property&quot; is always relative to the point of view of the agent &quot;myself&quot;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#behind -->

    <owl:ObjectProperty rdf:about="&oro;behind">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is behind</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;hasRelativePosition"/>
        <owl:inverseOf rdf:resource="&oro;inFrontOf"/>
        <owl:equivalentProperty rdf:resource="&concept;behind-Generally"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#belongsTo -->

    <owl:ObjectProperty rdf:about="&oro;belongsTo">
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <rdfs:label xml:lang="fr">appartient Ã </rdfs:label>
        <rdfs:range rdf:resource="&oro;Agent"/>
        <rdfs:domain rdf:resource="&oro;Artifact"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#bringsToStaticSituation -->

    <owl:ObjectProperty rdf:about="&oro;bringsToStaticSituation">
        <rdfs:label>leads to the static situation</rdfs:label>
        <rdfs:domain rdf:resource="&concept;Action"/>
        <rdfs:range rdf:resource="&concept;StaticSituation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#canBePerformedBy -->

    <owl:ObjectProperty rdf:about="&oro;canBePerformedBy">
        <rdfs:label>can be performed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#canPerform -->

    <owl:ObjectProperty rdf:about="&oro;canPerform">
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <owl:inverseOf rdf:resource="&oro;canBePerformedBy"/>
        <rdfs:range rdf:resource="&concept;PurposefulAction"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#canUndertakeNow -->

    <owl:ObjectProperty rdf:about="&oro;canUndertakeNow">
        <rdfs:label>can undertake now</rdfs:label>
        <rdfs:comment>Links an agent to an action it can starts now. When required, this relationship is to be computed by a planner (or inferred from a model of the human).</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;PurposefulAction"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#contains -->

    <owl:ObjectProperty rdf:about="&oro;contains">
        <rdfs:comment xml:lang="en">Links a (physical) container to its (physical) content.</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Container"/>
        <owl:inverseOf rdf:resource="&oro;isContainedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#currentlyBodilyDoes -->

    <owl:ObjectProperty rdf:about="&oro;currentlyBodilyDoes">
        <rdfs:comment>(currentlyBodilyDoes AGENT ACTION) means that the (embodied) AGENT is currently performing a non-intentional action ACTION.</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;EmbodiedAgent"/>
        <rdfs:subPropertyOf rdf:resource="&oro;currentlyDoes"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#currentlyDoes -->

    <owl:ObjectProperty rdf:about="&oro;currentlyDoes">
        <owl:inverseOf rdf:resource="&oro;currentlyDoneBy"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#currentlyDoneBy -->

    <owl:ObjectProperty rdf:about="&oro;currentlyDoneBy">
        <rdfs:comment xml:lang="en">Denotes that the DOES of the EVENT is currently doing it. It&#39;s a temporaly bound version of doneBy.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&concept;doneBy"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#currentlyPerforms -->

    <owl:ObjectProperty rdf:about="&oro;currentlyPerforms">
        <rdfs:label>currently performs</rdfs:label>
        <rdfs:comment>(currentlyPerforms AGENT ACTION) means that the AGENT is currently performing the intentional ACTION.</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">performedBy is the non-temporaly bound inverse property.

currentlyBodilyDo is another specialization of currentlyDo, for non intentional actions.</rdfs:seeAlso>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:subPropertyOf rdf:resource="&oro;currentlyDoes"/>
        <rdfs:range rdf:resource="&concept;Action"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#desires -->

    <owl:ObjectProperty rdf:about="&oro;desires">
        <rdfs:label xml:lang="en">desire</rdfs:label>
        <rdfs:label xml:lang="en">desires</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;Situation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#duration -->

    <owl:ObjectProperty rdf:about="&oro;duration">
        <rdfs:label>lasts</rdfs:label>
        <rdfs:range rdf:resource="&oro;TimeInterval"/>
        <rdfs:subPropertyOf rdf:resource="&oro;temporallyRelated"/>
        <rdfs:domain rdf:resource="&concept;TemporalThing"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#endingPoint -->

    <owl:ObjectProperty rdf:about="&oro;endingPoint">
        <rdfs:range rdf:resource="&oro;TimePoint"/>
        <rdfs:subPropertyOf rdf:resource="&oro;temporallyRelated"/>
        <rdfs:domain rdf:resource="&concept;Plan-ExpectedSituationType"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#experiences -->

    <owl:ObjectProperty rdf:about="&oro;experiences">
        <rdfs:label xml:lang="en">experiences</rdfs:label>
        <rdfs:label xml:lang="fr">vit</rdfs:label>
        <rdfs:comment xml:lang="en">describes a agent that currently *is* in a specific (static) situation</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <owl:inverseOf rdf:resource="&oro;isExperienced"/>
        <rdfs:range rdf:resource="&concept;StaticSituation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#focusesOn -->

    <owl:ObjectProperty rdf:about="&oro;focusesOn">
        <rdfs:label>focuses on</rdfs:label>
        <rdfs:comment xml:lang="en">An agent is said to focus on an object if and only if it looksAt it AND pointsAt it.</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <owl:inverseOf rdf:resource="&oro;isFocusOf"/>
        <rdfs:range rdf:resource="&concept;EnduringThing-Localized"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasBodyPart -->

    <owl:ObjectProperty rdf:about="&oro;hasBodyPart">
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&oro;BodyPart"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasCharacteristicLocation -->

    <owl:ObjectProperty rdf:about="&oro;hasCharacteristicLocation">
        <rdfs:range rdf:resource="&oro;Location"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasColor -->

    <owl:ObjectProperty rdf:about="&oro;hasColor">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>describe the main, significant color of the object.</rdfs:comment>
        <question xml:lang="en">What is the color?</question>
        <rdfs:range rdf:resource="&oro;Color"/>
        <rdfs:subPropertyOf rdf:resource="&oro;hasFeature"/>
        <owl:equivalentProperty rdf:resource="&oro;mainColorOfObject"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasFeature -->

    <owl:ObjectProperty rdf:about="&oro;hasFeature"/>
    


    <!-- http://kb.openrobots.org#hasGoal -->

    <owl:ObjectProperty rdf:about="&oro;hasGoal">
        <rdfs:label xml:lang="en">aims to</rdfs:label>
        <rdfs:label xml:lang="fr">vise Ã </rdfs:label>
        <rdfs:comment>Goal: Entity towards which something moves.

Example: John went &gt;home&lt;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;hasThematicRoleRelationWith"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasInHand -->

    <owl:ObjectProperty rdf:about="&oro;hasInHand">
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&oro;GraspableObject"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasInLeftHand -->

    <owl:ObjectProperty rdf:about="&oro;hasInLeftHand">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&oro;hasInHand"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasInRightHand -->

    <owl:ObjectProperty rdf:about="&oro;hasInRightHand">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&oro;hasInHand"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasPosture -->

    <owl:ObjectProperty rdf:about="&oro;hasPosture">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&oro;Posture"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasRelativePosition -->

    <owl:ObjectProperty rdf:about="&oro;hasRelativePosition">
        <rdfs:label>has relative position</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Location"/>
        <rdfs:range rdf:resource="&oro;Location"/>
        <rdfs:subPropertyOf rdf:resource="&oro;agentRelativeProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasShape -->

    <owl:ObjectProperty rdf:about="&oro;hasShape">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&oro;Shape"/>
        <rdfs:subPropertyOf rdf:resource="&oro;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasSize -->

    <owl:ObjectProperty rdf:about="&oro;hasSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&oro;hasFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#hasThematicRoleRelationWith -->

    <owl:ObjectProperty rdf:about="&oro;hasThematicRoleRelationWith">
        <rdfs:label xml:lang="fr">a une relation thÃ©matique avec</rdfs:label>
        <rdfs:label>has a role relation with</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#holds -->

    <owl:ObjectProperty rdf:about="&oro;holds">
        <rdfs:range rdf:resource="&concept;PartiallyTangible"/>
        <rdfs:subPropertyOf rdf:resource="&concept;holds-Underspecified"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#inFrontOf -->

    <owl:ObjectProperty rdf:about="&oro;inFrontOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is in front of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;hasRelativePosition"/>
        <owl:equivalentProperty rdf:resource="&concept;inFrontOf-Generally"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#involves -->

    <owl:ObjectProperty rdf:about="&oro;involves">
        <rdfs:label xml:lang="en">involves</rdfs:label>
        <rdfs:label xml:lang="fr">implique</rdfs:label>
        <rdfs:domain rdf:resource="&concept;Situation"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isAbleTo -->

    <owl:ObjectProperty rdf:about="&oro;isAbleTo">
        <rdfs:label xml:lang="en">am able to</rdfs:label>
        <rdfs:label xml:lang="en">are able to</rdfs:label>
        <rdfs:label xml:lang="en">can</rdfs:label>
        <rdfs:label xml:lang="en">is able to</rdfs:label>
        <rdfs:comment>(isAbleTo AGENT ACTION) says that AGENT it technically/physically able to achieve the ACTION. It does not mean that the agent can do it right now (see canUndertakeNow for this meaning). The ACTION is constrainted to purposeful actions: isAbleTo does not intend to express a fact like &quot;a stone is able to sink into the water&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;PurposefulAction"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isAbove -->

    <owl:ObjectProperty rdf:about="&oro;isAbove">
        <owl:equivalentProperty rdf:resource="&concept;above-Generally"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isAt -->

    <owl:ObjectProperty rdf:about="&oro;isAt">
        <rdfs:label xml:lang="en">am at</rdfs:label>
        <rdfs:label xml:lang="en">are at</rdfs:label>
        <rdfs:label xml:lang="en">is at</rdfs:label>
        <rdfs:range rdf:resource="&oro;Location"/>
        <rdfs:domain rdf:resource="&oro;Location"/>
        <owl:inverseOf rdf:resource="&oro;placeOf"/>
        <owl:equivalentProperty rdf:resource="&concept;objectFoundInLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isBelow -->

    <owl:ObjectProperty rdf:about="&oro;isBelow">
        <rdfs:label xml:lang="en">is under</rdfs:label>
        <rdfs:label xml:lang="fr">est sous</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Location"/>
        <rdfs:range rdf:resource="&oro;Location"/>
        <owl:inverseOf rdf:resource="&oro;isAbove"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isContainedIn -->

    <owl:ObjectProperty rdf:about="&oro;isContainedIn"/>
    


    <!-- http://kb.openrobots.org#isExperienced -->

    <owl:ObjectProperty rdf:about="&oro;isExperienced">
        <rdfs:subPropertyOf rdf:resource="&oro;staticSituationInvolves"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isFocusOf -->

    <owl:ObjectProperty rdf:about="&oro;isFocusOf">
        <rdfs:label>is in the focus of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isIn -->

    <owl:ObjectProperty rdf:about="&oro;isIn">
        <rdfs:label xml:lang="en">am in</rdfs:label>
        <rdfs:label xml:lang="en">are in</rdfs:label>
        <rdfs:label xml:lang="en">is in</rdfs:label>
        <rdfs:label xml:lang="fr">est dans</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;isAt"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isLocated -->

    <owl:ObjectProperty rdf:about="&oro;isLocated">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>has relative location</rdfs:label>
        <rdfs:comment>describe the symbolic position on an object relative to &#39;myself&#39;.
Values can be:
{NEAR_FAR}_{FRONT LEFT RIGHT BACK}</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isNextTo -->

    <owl:ObjectProperty rdf:about="&oro;isNextTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">am next to</rdfs:label>
        <rdfs:label xml:lang="en">are next to</rdfs:label>
        <rdfs:label xml:lang="en">is next to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#isOn -->

    <owl:ObjectProperty rdf:about="&oro;isOn">
        <rdfs:label xml:lang="en">is on</rdfs:label>
        <rdfs:label xml:lang="fr">est sur</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;isAt"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#leftOf -->

    <owl:ObjectProperty rdf:about="&oro;leftOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is left of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;hasRelativePosition"/>
        <owl:inverseOf rdf:resource="&oro;rightOf"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#looksAt -->

    <owl:ObjectProperty rdf:about="&oro;looksAt">
        <rdfs:label xml:lang="en">looks at</rdfs:label>
        <rdfs:label xml:lang="fr">regarde</rdfs:label>
        <rdfs:comment xml:lang="en">Describes the status of an agent actively looking at an object (&#39;sees&#39; doesn&#39;t imply the agent is actively looking at the object, but only that the object is in the field of view of the agent).</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;EnduringThing-Localized"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#mainColorOfObject -->

    <owl:ObjectProperty rdf:about="&oro;mainColorOfObject">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>has main color</rdfs:label>
        <openCycUri>http://sw.opencyc.org/concept/Mx4r45WD6H7_EdaAAAABAjVgzA</openCycUri>
        <interrogative xml:lang="en">What is the color?</interrogative>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#objectOfAction -->

    <owl:ObjectProperty rdf:about="&oro;objectOfAction">
        <rdfs:comment>Binds an action to one or several objects which are targetted by the action.
For instance, in the assertion, &quot;I take a cup&quot;, &quot;cup&quot; is the ObjectOfAction of &quot;to take&quot;.

In the sentence &quot;Jido shows the Eiffel tower to the tourist&quot;, the object of action is the Eiffel tower and the receiver is the tourist, as specified by the &quot;receiverOfAction&quot; property.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;actionInvolves"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#placeOf -->

    <owl:ObjectProperty rdf:about="&oro;placeOf"/>
    


    <!-- http://kb.openrobots.org#pointsAt -->

    <owl:ObjectProperty rdf:about="&oro;pointsAt">
        <rdfs:label>points at</rdfs:label>
        <rdfs:comment xml:lang="en">Describes the status of an agent physically pointing at a physical location with a hand.</rdfs:comment>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;EnduringThing-Localized"/>
        <owl:equivalentProperty rdf:resource="&concept;pointingToward"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#purposeOfAction -->

    <owl:ObjectProperty rdf:about="&oro;purposeOfAction">
        <rdfs:comment>binds an action to a state of the world (a Situation) which is desired as a result (at short or long term) of this action.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;actionInvolves"/>
        <rdfs:range rdf:resource="&concept;Situation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#reaches -->

    <owl:ObjectProperty rdf:about="&oro;reaches">
        <rdfs:comment>This property represent the &quot;active form&quot; of the &quot;isReachable&quot; predicate.
?o isReachable true &lt;=&gt; myself reaches ?o

It is meant to be only use through rule inference, and is added to this &quot;ROMAN&quot; scenario specific ontology to ensure fluid interaction with the Natural Language module.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#receivedBy -->

    <owl:ObjectProperty rdf:about="&oro;receivedBy">
        <rdfs:label xml:lang="en">received by</rdfs:label>
        <rdfs:label xml:lang="fr">reÃ§u par</rdfs:label>
        <rdfs:comment>Link an action to the object or agent that is a recipient of the action result.
Not to be confused with actionOnObject that denote that the object is the main concern of the action.

Example: John got &gt;Mary&lt; a present.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;hasThematicRoleRelationWith"/>
        <rdfs:domain rdf:resource="&concept;PurposefulAction"/>
        <rdfs:range rdf:resource="&concept;SpatialThing-Localized"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#recipientOfAction -->

    <owl:ObjectProperty rdf:about="&oro;recipientOfAction">
        <rdfs:comment>When relevant for the considered action, this property specify the agent or object whose state is eventually modified as a *result* of an action. It is not be the object of action itself (specified by the &quot;objectOfAction&quot; property).

For instance, in the sentence &quot;The robot gives the cup to the human&quot;, the human is the final recipient of the cup and hence the recipientOfAction. In the sentence &quot;I place the robot on the chair&quot;, the chair is the recipient of action while the robot is the object of the action. In the sentence &quot;Jido shows the Eiffel tower to the tourist&quot;, the recipient is the tourist and the object of action is the Eiffel tower.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;actionInvolves"/>
        <rdfs:range rdf:resource="&concept;PartiallyTangible"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#rightOf -->

    <owl:ObjectProperty rdf:about="&oro;rightOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is right of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&oro;hasRelativePosition"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#sees -->

    <owl:ObjectProperty rdf:about="&oro;sees">
        <rdfs:label xml:lang="en">see</rdfs:label>
        <rdfs:label xml:lang="en">sees</rdfs:label>
        <rdfs:label xml:lang="fr">voit</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Agent"/>
        <rdfs:range rdf:resource="&concept;EnduringThing-Localized"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#startingPoint -->

    <owl:ObjectProperty rdf:about="&oro;startingPoint">
        <rdfs:range rdf:resource="&oro;TimePoint"/>
        <rdfs:subPropertyOf rdf:resource="&oro;temporallyRelated"/>
        <rdfs:domain rdf:resource="&concept;Plan-ExpectedSituationType"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#startsAfterEndingOf -->

    <owl:ObjectProperty rdf:about="&oro;startsAfterEndingOf">
        <rdfs:comment xml:lang="en">A ComplexTemporalPredicate (q.v.) that relates any pair of temporal things such that the first starts after the second ends. (startsAfterEndingOf AFTER BEFORE) means that the time point (see TimePoint) at which AFTER first starts to exist or occur is temporally later than (see after) the time point at which BEFORE finally ceases to exist or occur. Thus it is equivalent to the form (after (StartFn AFTER) (EndFn BEFORE)). That is, the startingPoint of AFTER is later than the endingPoint of BEFORE.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&oro;after-Underspecified"/>
        <rdfs:domain rdf:resource="&concept;TemporalThing"/>
        <rdfs:range rdf:resource="&concept;TemporalThing"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#staticSituationInvolves -->

    <owl:ObjectProperty rdf:about="&oro;staticSituationInvolves">
        <rdfs:comment>Binds a static situation (a state of the world) to the agents involved in the situation.</rdfs:comment>
        <rdfs:range rdf:resource="&oro;Agent"/>
        <rdfs:subPropertyOf rdf:resource="&oro;involves"/>
        <rdfs:domain rdf:resource="&concept;StaticSituation"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#taskOfPlan -->

    <owl:ObjectProperty rdf:about="&oro;taskOfPlan">
        <rdfs:label>is a task of plan</rdfs:label>
        <rdfs:domain rdf:resource="&oro;Task"/>
        <rdfs:range rdf:resource="&concept;Plan-ExpectedSituationType"/>
    </owl:ObjectProperty>
    


    <!-- http://kb.openrobots.org#temporallyRelated -->

    <owl:ObjectProperty rdf:about="&oro;temporallyRelated"/>
    


