                                <xsd:maxInclusive rdf:datatype="&xsd;integer">12</xsd:maxInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="&xsd;integer">7</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
