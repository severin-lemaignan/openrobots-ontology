    </owl:DatatypeProperty>
    


    <!-- http://kb.openrobots.org#isTraversable -->

    <owl:DatatypeProperty rdf:about="&oro;isTraversable">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&concept;EnduringThing-Localized"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


