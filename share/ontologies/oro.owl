        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://kb.openrobots.org#isMovable -->

    <owl:DatatypeProperty rdf:about="&oro;isMovable">
