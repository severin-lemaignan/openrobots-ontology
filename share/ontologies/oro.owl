        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&oro;hasCharacteristicDimension"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="&xsd;integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="&xsd;integer">50</xsd:minInclusive>
                            </rdf:Description>
